<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">


    <!-- Uberdust model definition -->
    <bean id="Capability" class="eu.wisebed.wisedb.model.Capability"/>
    <bean id="Position" class="eu.wisebed.wisedb.model.Position"/>
    <bean id="Node" class="eu.wisebed.wisedb.model.Node"/>
    <bean id="Link" class="eu.wisebed.wisedb.model.Link"/>
    <bean id="NodeReading" class="eu.wisebed.wisedb.model.NodeReading"/>
    <bean id="LinkReading" class="eu.wisebed.wisedb.model.LinkReading"/>
    <bean id="NodeCapability" class="eu.wisebed.wisedb.model.NodeCapability"/>
    <bean id="LinkCapability" class="eu.wisebed.wisedb.model.LinkCapability"/>
    <bean id="Setup" class="eu.wisebed.wisedb.model.Setup"/>
    <bean id="Testbed" class="eu.wisebed.wisedb.model.Testbed"/>
    <bean id="Origin" class="eu.wisebed.wisedb.model.Origin"/>
    <bean id="TimeInfo" class="eu.wisebed.wisedb.model.TimeInfo"/>
    <bean id="LastNodeReading" class="eu.wisebed.wisedb.model.LastNodeReading"/>
    <bean id="LastLinkReading" class="eu.wisebed.wisedb.model.LastLinkReading"/>
    <bean id="Schedule" class="eu.wisebed.wisedb.model.Schedule"/>

</beans>
